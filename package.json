{
  "name": "@terenceodonoghue/react-hooks",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Terence O'Donoghue <1697905+terenceodonoghue@users.noreply.github.com>",
  "packageManager": "yarn@3.1.1",
  "main": "./dist/cjs/index.js",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "prepack": "yarn concurrently \"yarn:prepack:*\"",
    "prepack:cjs": "tsc -p tsconfig-cjs.json",
    "prepack:mjs": "tsc -p tsconfig-mjs.json",
    "prepare": "rimraf ./dist",
    "storybook": "start-storybook -p 6006 --docs",
    "build-storybook": "build-storybook --docs"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-docs": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "eslint-plugin-mdx": "^1.16.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
