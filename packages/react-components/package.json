{
  "name": "@terenceodonoghue/react-components",
  "version": "5.0.0",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/terenceodonoghue/react.git",
    "directory": "packages/react-components"
  },
  "type": "module",
  "exports": {
    "./core": "./dist/core/index.js",
    "./velocity": "./dist/velocity/index.js",
    "./velocity.css": "./dist/velocity/index.css"
  },
  "typesVersions": {
    "*": {
      "core": [
        "./dist/core/index.d.ts"
      ],
      "velocity": [
        "./dist/velocity/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn concurrently \"yarn:build:*\"",
    "build:css": "postcss src/**/*.css --base src/ -d dist/ --no-map",
    "build:js": "tsc",
    "clean": "rimraf ./dist",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path ../../.eslintignore --max-warnings=0 ./",
    "prepack": "yarn build",
    "prepublish": "yarn lint"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@terenceodonoghue/react-icons": "^2.1.1",
    "csstype": "^3.1.0",
    "facepaint": "^1.2.1",
    "lodash-es": "^4.17.21",
    "polished": "^4.2.2",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "packageManager": "yarn@4.0.0-rc.11",
  "engines": {
    "node": ">=14.16"
  }
}
